{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/esp/esp-idf/**",
                "~/esp/esp-idf/components/**",
                "~/esp/esp-idf/components/esp_wifi/include",
                "~/esp/esp-idf/components/esp_common/include",
                "~/esp/esp-idf/components/esp_system/include",
                "~/esp/esp-idf/components/freertos/**",
                "~/esp/esp-idf/components/freertos/FreeRTOS-Kernel/include",
                "~/esp/esp-idf/components/freertos/esp_additions/include/freertos",
                "~/esp/esp-idf/components/freertos/FreeRTOS-Kernel/portable/xtensa/include",
                "~/esp/esp-idf/components/hal/include",
                "~/esp/esp-idf/components/soc/esp32/include"
            ],
            "defines": [
                "ESP_PLATFORM",
                "IDF_VER=\"v5.3.2\"",
                "ESP32",
                "_GNU_SOURCE"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}